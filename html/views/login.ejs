
<!DOCTYPE html>
<html lang="en">
<head>
    <title>My Feedback</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="/cssFiles/global.css">
</head>

<body class="login" id = 'mainBody'>
	<% include header.ejs %> 
	<div class="container">
		<h1 class="redtext" align="center" >Welcome to My Feedback Portal</h1>
	</div>
	
	<div class="container-fluid">
		<div< class="row">
		   <div class="col-md-4 col-sm-4 col-xs-12"></div>
		   <div class="col-md-4 col-sm-4 col-xs-12">
				<form class="form-container" action="/dashboard" method="post">
					<h1 class="redtext" align="center">Login Here</h1>
				  <div class="form-group">
				    <label class = "control-label" for="exampleInputEmail1">Email address</label>
				    <input type="email" class="form-control" id="email" name='emailid' aria-describedby="emailHelp" placeholder="Enter email">
				    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
				  </div>
				  <div class="form-group">
				    <label class = "control-label" for="exampleInputPassword1">Password</label>
				    <input type="password" class="form-control" name = 'password' id="password" placeholder="Password">
				  </div>
				  <div class="form-check">
				    <input type="checkbox" class="form-check-input" id="exampleCheck1">
				    <label class="control-label" for="exampleCheck1">Check me out</label>
				  </div>
				  <button type="submit" id='loginSubmit' class="btn btn-primary" >Submit</button>
				  <text class="redtext"><%= ret %></text>
				</form>
			</div>
		<div class="col-md-4 col-sm-4 col-xs-12"></div>
	</div<>
</div>

<script>
	document.getElementById("loginNav").classList.add('active');
</script>

</body>
</html>